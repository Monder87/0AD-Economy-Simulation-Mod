<?xml version="1.0" encoding="utf-8"?>
<particles>

    <texture>art/textures/particles/dust_256a.png</texture>
    <blend mode="over"/>

    <constant name="emissionrate" value="20.0"/>
    <uniform name="lifetime" min="10.0" max="15.0"/>

    <uniform name="position.x" min="-4.0" max="4.0"/>
    <uniform name="position.z" min="-4.0" max="4.0"/>
    <constant name="position.y" value="5.0"/>

    <uniform name="angle" min="-3.14" max="3.14"/>

    <uniform name="velocity.x" min="0.0" max="0.5"/>
    <uniform name="velocity.y" min="2.0" max="3.5"/>
    <uniform name="velocity.z" min="-0.5" max="0.5"/>
    <uniform name="velocity.angle" min="-1.0" max="1.0"/>

    <uniform name="size" min="6.5" max="10.5"/>

    <uniform name="color.r" min="0.4" max="0.8"/>
    <copy name="color.g" from="color.r"/>
    <copy name="color.b" from="color.r"/>

</particles>
